				<OutputValue name="energyLimitedEvaporation" precision ="3">448.240</OutputValue>
				<OutputValue name="soilEvaporation" precision ="3">448.240</OutputValue>
				<OutputValue name="soilHeatFlux" precision ="3">188.817</OutputValue>
        	
        	</Test>

        </Testset>
      </Testsets>
 	
</ModelUnit>
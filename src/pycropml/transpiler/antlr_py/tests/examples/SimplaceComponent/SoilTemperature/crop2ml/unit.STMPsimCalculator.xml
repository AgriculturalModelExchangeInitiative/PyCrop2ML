<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ModelUnit PUBLIC " " "https://raw.githubusercontent.com/AgriculturalModelExchangeInitiative/crop2ml/master/ModelUnit.dtd">
<ModelUnit modelid="Simplace.SoilTemp.STMPsimCalculator" name="STMPsimCalculator" timestep="1" version="001">
    <Description>        
        <Title>STMPsimCalculator model</Title>
        <Authors>Gunther Krauss</Authors>
        <Institution>INRES Pflanzenbau, Uni Bonn</Institution>
        <Reference>https://simplace.net/doc/simplace_modules/class_net.simplace.sim.components.soil.temperature.STMPsimCalculator.html#class_net.simplace.sim.components.soil.temperature.STMPsimCalculator</Reference>
        <Abstract>as given in the documentation</Abstract>        
    </Description>    
    <Inputs>        
        <Input datatype="DOUBLEARRAY" default="" description="Depth of soil layer" inputtype="variable" max="20" min="0.03" name="cSoilLayerDepth" parametercategory="constant" unit="http://www.wurvoc.org/vocabularies/om-1.8/metre"/>
        <Input datatype="DOUBLE" default="" description="Mean temperature on first day" inputtype="variable" max="50" min="-40" name="cFirstDayMeanTemp" parametercategory="constant" unit="http://www.wurvoc.org/vocabularies/om-1.8/degree_Celsius"/>
        <Input datatype="DOUBLE" default="" description="Constant Temperature of deepest soil layer" inputtype="variable" max="20" min="-10" name="cAVT" parametercategory="constant" unit="http://www.wurvoc.org/vocabularies/om-1.8/degree_Celsius"/>
        <Input datatype="DOUBLE" default="2" description="Mean bulk density" inputtype="variable" max="4" min="1" name="cABD" parametercategory="constant" unit="http://www.wurvoc.org/vocabularies/om-1.8/tonne_per_cubic_metre"/>
        <Input datatype="DOUBLE" default="6" description="Initial value for damping depth of soil" inputtype="variable" max="20" min="1.5" name="cDampingDepth" parametercategory="constant" unit="http://www.wurvoc.org/vocabularies/om-1.8/metre"/>
        <Input datatype="BOOLEAN" default="false" description="Switch to re-initialize the model with initial values." inputtype="variable" max="" min="" name="iDoInitialize" unit="http://www.wurvoc.org/vocabularies/om-1.8/one" variablecategory="exogenous"/>
        <Input datatype="DOUBLE" default="5" description="Content of water in Soil" inputtype="variable" max="20" min="1.5" name="iSoilWaterContent" unit="http://www.wurvoc.org/vocabularies/om-1.8/millimetre" variablecategory="exogenous"/>
        <Input datatype="DOUBLE" default="" description="Temperature at soil surface" inputtype="variable" max="20" min="1.5" name="iSoilSurfaceTemperature" unit="http://www.wurvoc.org/vocabularies/om-1.8/degree_Celsius" variablecategory="exogenous"/>
        <Input datatype="DOUBLEARRAY" default="" description="Depth of soil layer plus additional depth" inputtype="variable" max="20" min="0.03" name="pSoilLayerDepth" unit="http://www.wurvoc.org/vocabularies/om-1.8/metre" variablecategory="auxiliary"/>
    </Inputs>
    <Outputs>
        <Output datatype="DOUBLEARRAY" description="Array of temperature " max="40" min="-20" name="SoilTempArray" unit="http://www.wurvoc.org/vocabularies/om-1.8/degree_Celsius" variablecategory="auxiliary"/>
    </Outputs>
    <Initialization name="init_STMPsimCalculator" filename="algo/pyx/init.STMPsimCalculator.pyx" language="cyml" />
    <Algorithm filename="algo/pyx/STMPsimCalculator.pyx" language="cyml" platform=""/>
    <Parametersets>
        <Parameterset description="" name="Param1"/>
    </Parametersets>
    <Testsets>
        <Testset description="" name="" parameterset="Param1">
            <Test name="test1">
                <InputValue name=""></InputValue>
                <OutputValue name=""></OutputValue>
            </Test>
        </Testset>
    </Testsets>
</ModelUnit>
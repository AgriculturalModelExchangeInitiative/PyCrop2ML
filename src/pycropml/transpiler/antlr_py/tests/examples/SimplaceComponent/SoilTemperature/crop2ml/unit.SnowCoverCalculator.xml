<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ModelUnit PUBLIC " " "https://raw.githubusercontent.com/AgriculturalModelExchangeInitiative/crop2ml/master/ModelUnit.dtd">
<ModelUnit modelid="Simplace.SoilTemp.SnowCoverCalculator" name="SnowCoverCalculator" timestep="1" version="001">
    <Description>
        <Title>SnowCoverCalculator model</Title>
        <Authors>Gunther Krauss</Authors>
        <Institution>INRES Pflanzenbau, Uni Bonn</Institution>
        <Reference>(&apos;http://www.simplace.net/doc/simplace_modules/&apos;,)</Reference>
        <Abstract>as given in the documentation</Abstract>
    </Description>
    <Inputs>
        <Input datatype="DOUBLE" default="0.5" description="Carbon content of upper soil layer" inputtype="parameter" max="20" min="0" name="CARBON" parametercategory="constant" unit="http://www.wurvoc.org/vocabularies/om-1.8/percent"/>
        <Input datatype="BOOLEAN" default="false" description="Switch to re-initialize the model with initial values." inputtype="variable" max="null" min="null" name="iDoInitialize" unit="http://www.wurvoc.org/vocabularies/om-1.8/one" variablecategory="exogenous"/>
        <Input datatype="DOUBLE" default="" description="Daily maximum temperature" inputtype="variable" max="50" min="-40" name="TMX" unit="http://www.wurvoc.org/vocabularies/om-1.8/degree_Celsius" variablecategory="exogenous"/>
        <Input datatype="DOUBLE" default="" description="Daily minimum temperature" inputtype="variable" max="50" min="-40" name="TMN" unit="http://www.wurvoc.org/vocabularies/om-1.8/degree_Celsius" variablecategory="exogenous"/>
        <Input datatype="DOUBLE" default="" description="Solar radiation" inputtype="variable" max="2000" min="0" name="RA" unit="http://www.wurvoc.org/vocabularies/om-1.8/megajoule_per_square_metre" variablecategory="exogenous"/>
        <Input datatype="DOUBLE" default="" description="Rain amount" inputtype="variable" max="60" min="0" name="PREC" unit="http://www.wurvoc.org/vocabularies/om-1.8/millimetre" variablecategory="exogenous"/>
        <Input datatype="DOUBLE" default="" description="Crop residues plus above ground biomass" inputtype="variable" max="20000" min="0" name="CV" unit="http://www.wurvoc.org/vocabularies/om-1.8/gram_per_square_metre" variablecategory="exogenous"/>
        <Input datatype="DOUBLE" default="" description="Potenial Evaporation" inputtype="variable" max="12" min="0" name="PEVAP" unit="http://www.wurvoc.org/vocabularies/om-1.8/millimetre" variablecategory="exogenous"/>
        <Input datatype="DOUBLE" default="" description="Leaf area index" inputtype="variable" max="10" min="0" name="SLA" unit="http://www.wurvoc.org/vocabularies/om-1.8/square_metre_per_square_metre" variablecategory="exogenous"/>
        <Input datatype="DOUBLEARRAY" default="" description="Soil Temp array of last day" inputtype="variable" max="35" min="-15" name="TSOIL" unit="http://www.wurvoc.org/vocabularies/om-1.8/degree_Celsius" variablecategory="exogenous"/>
        <Input datatype="DOUBLE" default="" description="Albedo" inputtype="variable" max="1" min="0" name="Albedo" unit="http://www.wurvoc.org/vocabularies/om-1.8/one" variablecategory="auxiliary"/>
    </Inputs>
    <Outputs>
        <Output datatype="DOUBLE" description="Snow water content" max="1500" min="0" name="SNOWC" unit="http://www.wurvoc.org/vocabularies/om-1.8/millimetre" variablecategory="auxiliary"/>
        <Output datatype="DOUBLE" description="Snow isolation index" max="1" min="0" name="BCV" unit="http://www.wurvoc.org/vocabularies/om-1.8/one" variablecategory="auxiliary"/>
        <Output datatype="DOUBLE" description="Soil surface temperature" max="70" min="-40" name="DST0" unit="http://www.wurvoc.org/vocabularies/om-1.8/degree_Celsius" variablecategory="auxiliary"/>
        <Output datatype="INT" description="Age of snow" max="null" min="0" name="AgeOfSnow" unit="http://www.wurvoc.org/vocabularies/om-1.8/one" variablecategory="auxiliary"/>
    </Outputs>
    <Initialization name="init_SnowCoverCalculator" filename="algo/pyx/init.SnowCoverCalculator.pyx" language="cyml" />
    <Algorithm filename="algo/pyx/SnowCoverCalculator.pyx" language="cyml" platform=""/>
    <Parametersets>
        <Parameterset description="" name="Param1"/>
    </Parametersets>
    <Testsets>
        <Testset description="" name="" parameterset="Param1">
            <Test name="test1">
                <InputValue name=""></InputValue>
                <OutputValue name=""></OutputValue>
            </Test>
        </Testset>
    </Testsets>
</ModelUnit>
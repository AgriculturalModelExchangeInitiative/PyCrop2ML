<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ModelUnit PUBLIC " " "https://raw.githubusercontent.com/AgriculturalModelExchangeInitiative/crop2ml/master/ModelUnit.dtd">
<ModelUnit modelid="SQ.EnergyBalance.Penman" name="Penman" timestep="1" version="1.0">
    <Description>
        <Title>Penman Model</Title>
        <Authors>Peter D. Jamieson, Glen S. Francis, Derick R. Wilson, Robert J. Martin</Authors>
        <Institution>New Zealand Institute for Crop and Food Research Ltd.,</Institution>
        <URI></URI>
        <Reference>https://doi.org/10.1016/0168-1923</Reference>
        <ExtendedDescription></ExtendedDescription>
        <ShortDescription></ShortDescription></Description>
    <Inputs>
        <Input name="evapoTranspirationPriestlyTaylor" description="evapoTranspiration of Priestly Taylor " inputtype="variable" variablecategory="rate" datatype="DOUBLE" max="10000" min="0" default="449.367" unit="g m-2 d-1"/>
        <Input name="hslope" description="the slope of saturated vapor pressure temperature curve at a given temperature " inputtype="variable" variablecategory="auxiliary" datatype="DOUBLE" max="1000" min="0" default="0.584" unit="hPa "/>
        <Input name="VPDair" description="vapour pressure density" inputtype="variable" variablecategory="auxiliary" datatype="DOUBLE" max="1000" min="0" default="2.19" unit="hPa"/>
        <Input name="psychrometricConstant" description="psychrometric constant" inputtype="parameter" parametercategory="constant" datatype="DOUBLE" max="1" min="0" default="0.66" unit=""/>
        <Input name="Alpha" description="Priestley-Taylor evapotranspiration proportionality constant" inputtype="parameter" parametercategory="constant" datatype="DOUBLE" max="100" min="0" default="1.5" unit=""/>
        <Input name="lambdaV" description="latent heat of vaporization of water" inputtype="parameter" parametercategory="constant" datatype="DOUBLE" max="10" min="0" default="2.454" unit=""/>
        <Input name="rhoDensityAir" description="Density of air" inputtype="parameter" parametercategory="constant" datatype="DOUBLE" max="None" min="None" default="1.225" unit=""/>
        <Input name="specificHeatCapacityAir" description="Specific heat capacity of dry air" inputtype="parameter" parametercategory="constant" datatype="DOUBLE" max="1" min="0" default="0.00101" unit=""/>
        <Input name="conductance" description="conductance" inputtype="variable" variablecategory="state" datatype="DOUBLE" max="10000" min="0" default="598.685" unit="m d-1"/></Inputs>
    <Outputs>
        <Output name="evapoTranspirationPenman" description="evapoTranspiration of Penman Monteith" datatype="DOUBLE" variablecategory="rate" max="5000" min="0" unit="g m-2 d-1"/></Outputs>
    <Algorithm language="cyml" platform="" filename="algo/pyx/Penman.pyx"/>
    <Parametersets/>
    <Testsets/></ModelUnit>